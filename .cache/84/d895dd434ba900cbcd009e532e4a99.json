:81,"column":0},"generated":{"line":89,"column":42}},{"source":"src/components/Books.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":89,"column":46}},{"source":"src/components/Books.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":89,"column":47}},{"source":"src/components/Books.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":89,"column":56}},{"source":"src/components/Books.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":89,"column":57}},{"source":"src/components/Books.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":89,"column":62}},{"source":"src/components/Books.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":89,"column":67}},{"source":"src/components/Books.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":89,"column":68}},{"source":"src/components/Books.vue","name":null,"original":{"line":82,"column":0},"generated":{"line":90,"column":0}},{"source":"src/components/Books.vue","name":null,"original":{"line":68,"column":0},"generated":{"line":91,"column":0}},{"source":"src/components/Books.vue","name":null,"original":{"line":52,"column":0},"generated":{"line":92,"column":0}}],"sources":{"src/components/Books.vue":"<template>\r\n    <div>\r\n        <section class=\"book-form-section\">\r\n            <form class=\"form\" @submit.prevent=\"addBook\">\r\n                <div class=\"form-field\">\r\n                    <label for=\"book-title\" class=\"book-label\">Title:</label>\r\n                    <input type=\"text\" class=\"book-input\" id=\"book-title\" required v-model=\"book.title\">\r\n                </div>\r\n                <div class=\"form-field\">\r\n                    <label for=\"book-author\" class=\"book-label\">Author:</label>\r\n                    <input type=\"text\" class=\"book-input\" id=\"book-author\" required v-model=\"book.author\">\r\n                </div>\r\n                <div class=\"form-field\">\r\n                    <label for=\"book-pages\" class=\"book-label\">Pages:</label>\r\n                    <input type=\"number\" class=\"book-input\" id=\"book-pages\" required min=\"1\" v-model=\"book.pages\">\r\n                </div>\r\n                <div class=\"form-field\">\r\n                    <label for=\"book-read\" class=\"book-label\">Read Yet?</label>\r\n                    <input type=\"checkbox\" class=\"book-input book-input-checkbox\" id=\"book-read\" v-model=\"book.status\">\r\n                </div>\r\n                <button class=\"form-add-btn\" type=\"submit\">Add Book</button>\r\n            </form>\r\n        </section>\r\n        <section class=\"book-table-section\">\r\n            <table class=\"book-table\">\r\n                <thead class=\"thead\">\r\n                    <tr class=\"thead-row\" id=\"thead-row\">\r\n                        <th class=\"th\">Title</th>\r\n                        <th class=\"th\">Author</th>\r\n                        <th class=\"th\">Pages</th>\r\n                        <th class=\"th\">Status (toggle)</th>\r\n                        <th class=\"th\">Delete</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody class=\"book-list\">\r\n                    <!-- injected books here -->\r\n                    <tr v-for=\"(value, key) in books\" v-bind:key=\"key\">\r\n                        <td>{{ value.title }}</td>\r\n                        <td>{{ value.author }}</td>\r\n                        <td>{{ value.pages }}</td>\r\n                        <td v-if=\"value.status\"><button class=\"status-btn finished\" v-on:click=\"value.status = !value.status\">Finished!</button></td>\r\n                        <td v-else><button class=\"status-btn unfinished\" v-on:click=\"value.status = !value.status\">Not yet</button></td>\r\n                        <td><img class=\"delete-btn\" src=\"../images/trashcan.svg\" alt=\"Trashcan icon\" v-on:click=\"removeBook\"></td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </section>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"Books\",\r\n    data() {\r\n        return {\r\n            book: {\r\n                title: \"\",\r\n                author: \"\",\r\n                pages: null,\r\n                status: false\r\n            },\r\n            books: []\r\n        }\r\n    },\r\n    created() {\r\n        this.books = JSON.parse(localStorage.getItem(\"storedBooks\")) || [];\r\n    },\r\n    methods: {\r\n        addBook() {\r\n            this.books.push({title: this.book.title, author: this.book.author, pages: this.book.pages, status: this.book.status});\r\n            this.book.title = \"\";\r\n            this.book.author = \"\";\r\n            this.book.pages = null;\r\n            this.book.status = false;\r\n\r\n            localStorage.setItem(\"storedBooks\", JSON.stringify(this.books));\r\n        },\r\n        removeBook() {\r\n            this.books.splice(this, 1);\r\n\r\n            localStorage.setItem(\"storedBooks\", JSON.stringify(this.books));\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n/* Book Form Section */\r\n.form {\r\n    width: 960px;\r\n    max-width: 90%;\r\n    margin: 2em auto;\r\n}\r\n\r\n.form-field {\r\n    display: flex;\r\n    flex-direction: column;\r\n    margin-top: 1em;\r\n    margin-bottom: 1em;\r\n}\r\n\r\n.form-add-btn {\r\n    display: block;\r\n    width: 100%;\r\n    padding: .75em;\r\n    cursor: pointer;\r\n    border: none;\r\n    border-radius: 4px;\r\n    background-color: #2286e4fd;\r\n    color: #fff;\r\n    font-size: 1.1rem;\r\n    outline: none;\r\n\r\n    transition:  background-color 210ms ease;\r\n}\r\n\r\n.form-add-btn:hover {\r\n    background-color: #1a66adfd;\r\n}\r\n\r\n.book-label {\r\n    margin-bottom: .5em;\r\n    font-weight: bolder;\r\n}\r\n\r\n.book-input {\r\n    padding-top: .25em;\r\n    padding-bottom: .25em;\r\n    padding-left: .35em;\r\n    font-size: 1.1rem;\r\n}\r\n\r\n.book-input-checkbox {\r\n    cursor: pointer;\r\n    width: 1em;\r\n    height: 1em;\r\n}\r\n\r\n/* Book List Table */\r\n.book-table {\r\n    width: 960px;\r\n\r\n    margin: 0 auto;\r\n    margin-top: 2em;\r\n    margin-bottom: 3em;\r\n    padding: .5em;\r\n    border-top: 1px solid #ccc;\r\n    border-bottom: 1px solid #ccc;\r\n    table-layout: fixed;\r\n    border-collapse: collapse;\r\n}\r\n\r\nth,\r\ntd {\r\n    padding: 1em;\r\n}\r\n\r\nth {\r\n    text-align: left;\r\n}\r\n\r\n#thead-row {\r\n    background-color: #fbfbfb;\r\n}\r\n\r\ntr:nth-child(odd) {\r\n    background-color: #ddd;\r\n}\r\n\r\n.status-btn {\r\n    padding: .5em 1.25em;\r\n    border: none;\r\n    border-radius: 4px;\r\n    background-color: #008000;\r\n    color: #fff;\r\n    outline: none;\r\n    cursor: pointer;\r\n}\r\n\r\n.unfinished {\r\n    background-color: #cf5301;\r\n}\r\n\r\n.delete-btn {\r\n    width: 35px;\r\n    cursor: pointer;\r\n    transition: opacity 150ms ease;\r\n}\r\n\r\n.delete-btn:hover {\r\n    opacity: .7;\r\n}\r\n\r\n@media screen and (max-width: 66.56em) {\r\n    .book-table {\r\n        width: 90%;\r\n    }\r\n}\r\n\r\n@media screen and (max-width: 47em) {\r\n    .book-table {\r\n        font-size: .75rem;\r\n    }\r\n\r\n    th,\r\n    td {\r\n        padding: .75em;\r\n    }\r\n\r\n    .status-btn {\r\n        padding: .2em .5em;\r\n    }\r\n}\r\n\r\n</style>\r\n"},"lineCount":null}},"error":null,"hash":"93c06af2fdf2821140f929c0135bcbf0","cacheData":{"env":{}}}